<!DOCTYPE html>
<html>
<head>
	<title>Student Form</title>
</head>
<body>
	<%=form_tag students_path do %>
	<label>Student first name:</label>
	<%= text_field_tag :"first_name"%><br>
	<label>Student last name:</label>
	<%= text_field_tag :"last_name"%><br>
	<%= submit_tag "Submit Student"%>
	<%end%>

</body>
</html>